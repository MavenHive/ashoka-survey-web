<h1> <%= t ".publish_survey" %> </h1>

<%= semantic_form_for @survey, :url => survey_publish_to_users_path(@survey.id) do |f| %>
<%= f.input :users, :as => :check_boxes, :collection => @users, :member_label => Proc.new { |u| u["name"] }, :member_value => Proc.new { |u| u["id"] }, :label => "Choose users to publish to:" %>
	<%= f.actions do %>
     <%= f.action :submit, :as => :button %>
  <% end %>
<% end %>